exports[`getJORF JORFTEXT000036386252 1`] = `"{\\"id\\":\\"JORFTEXT000036386252\\",\\"type\\":\\"texte\\",\\"data\\":{\\"id\\":\\"LEGITEXT000036532184\\",\\"nature\\":\\"ARRETE\\",\\"titre\\":\\"Arrêté du 29 décembre 2017\\",\\"titrefull\\":\\"Arrêté du 29 décembre 2017 fixant la date d'entrée en vigueur des dispositions de l'article 4 du décret n° 2017-1724 du 20 décembre 2017 relatif à la mise en œuvre des ruptures d'un commun accord dans le cadre d'un accord collectif\\",\\"titrefull_s\\":null,\\"etat\\":\\"VIGUEUR\\",\\"date_debut\\":\\"2018-01-06\\",\\"date_fin\\":\\"2999-01-01\\",\\"autorite\\":null,\\"ministere\\":null,\\"num\\":null,\\"num_sequence\\":6,\\"nor\\":\\"MTRD1736794A\\",\\"date_publi\\":\\"2018-01-05\\",\\"date_texte\\":\\"2017-12-29\\",\\"derniere_modification\\":\\"2018-01-13\\",\\"origine_publi\\":\\"JORF n°0003 du 5 janvier 2018\\",\\"page_deb_publi\\":null,\\"page_fin_publi\\":null,\\"visas\\":\\"<p align=\\\\\\"left\\\\\\"><br/>La ministre du travail,<br/>Vu le code du travail, notamment ses articles L. 1237-19 et suivants et D. 1237-7 à D. 1233-12 ;<br/>Vu l'article 4 du décret n° 2017-1724 du 20 décembre 2017 relatif à la mise en œuvre des ruptures d'un commun accord dans le cadre d'un accord collectif,<br/>Arrête :</p>\\",\\"signataires\\":\\"<div align=\\\\\\"left\\\\\\"><p>Fait le 29 décembre 2017.</p></div><div align=\\\\\\"left\\\\\\"><p>Pour la ministre et par délégation :</p><p>La déléguée générale à l'emploi et à la formation professionnelle,</p><p>C. Chevrier</p></div>\\",\\"tp\\":null,\\"nota\\":null,\\"abro\\":null,\\"rect\\":null,\\"dossier\\":\\"TNC_en_vigueur\\",\\"cid\\":\\"JORFTEXT000036386252\\",\\"mtime\\":1516733678,\\"texte_id\\":null},\\"children\\":[{\\"id\\":\\"LEGIARTI000036534472\\",\\"type\\":\\"article\\",\\"data\\":{\\"id\\":\\"LEGIARTI000036534472\\",\\"section\\":null,\\"num\\":\\"1\\",\\"etat\\":\\"VIGUEUR\\",\\"date_debut\\":\\"2018-01-13\\",\\"date_fin\\":\\"2999-01-01\\",\\"type\\":\\"AUTONOME\\",\\"nota\